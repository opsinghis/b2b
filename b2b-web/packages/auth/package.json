  "devDependencies": {
    "@b2b/config": "workspace:*",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "next": "14.1.4",
    "react": "^18.2.0",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  }
