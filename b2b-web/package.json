{
  "name": "b2b-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:portal": "turbo run dev --filter=@b2b/portal",
    "dev:admin": "turbo run dev --filter=@b2b/admin",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:unit": "turbo run test",
    "test:coverage": "turbo run test -- --coverage",
    "test:e2e": "playwright test --project=e2e",
    "test:e2e:portal": "playwright test --project=e2e-portal",
    "test:e2e:admin": "playwright test --project=e2e-admin",
    "test:a11y": "playwright test --project=accessibility",
    "test:a11y:portal": "playwright test --project=a11y-portal",
    "test:a11y:admin": "playwright test --project=a11y-admin",
    "test:all": "pnpm test && pnpm test:e2e && pnpm test:a11y",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@changesets/cli": "^2.27.1",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^28.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turbo": "^2.0.0",
    "vitest": "^1.4.0"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
